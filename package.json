{
  "name": "aiinfo-electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build &&DEBUG=electron-builder electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "rebuild": "electron-rebuild",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.aiinfo.browser",
    "productName": "x-browser",
    "asar": true,
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "**/node_modules/impit/**",
      "**/node_modules/@impit/**",
      "**/node_modules/camoufox-js/**"
    ],
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "node_modules"
    ],
    "mac": {
      "target": "dmg",
      "defaultArch": "arm64"
    }
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.4.1",
    "camoufox-js": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "fingerprint-generator": "^2.1.76",
    "generative-bayesian-network": "^2.1.76",
    "header-generator": "^2.1.76",
    "impit": "^0.7.1",
    "js-yaml": "^4.1.0",
    "language-subtag-registry": "^0.3.23",
    "language-tags": "^2.1.0",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.546.0",
    "maxmind": "^5.0.1",
    "playwright-core": "^1.56.1",
    "progress": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.8.1",
    "ua-parser-js": "^2.0.6",
    "vali-date": "^1.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^24.8.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.1",
    "electron-builder": "^26.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prebuild-install": "^7.1.3",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}

